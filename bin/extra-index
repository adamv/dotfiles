#!/usr/bin/env python
import sys
import plistlib

def main():
    which_extra = sys.argv[1].lower()

    plist = plistlib.readPlist(sys.stdin)
    extras = plist['menuExtras']
    if not extras:
        print -1
        return
        
    for i, x in enumerate(extras):
        if x.lower().endswith(which_extra + ".menu"):
            print i
            return

if __name__ == '__main__':
    main()
