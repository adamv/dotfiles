#!/usr/bin/env bash

if [[ -z $1 ]]; then
    echo "No changelist or deploy workspace specified."
    echo "Usage: get-deploy.sh [name of deploy workspace] changelist"
    exit 1
fi

client="adamv-deploy"

if [[ -z $2 ]]; then
    revision=$1
else
    client=$1
    revision=$2
fi


p4 -c$client sync "//$client/...@$revision"
